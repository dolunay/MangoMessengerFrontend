{
	"name": "mango-angular-frontend",
	"version": "0.0.1",
	"main": "app.js",
	"scripts": {
		"start": "node server.js",
		"build-dev": "ng build --aot --configuration development",
		"build-qa": "ng build --aot --configuration quality-assurance",
		"build": "ng build --aot --configuration production",
		"test": "ng test",
		"e2e": "ng e2e",
		"lint": "eslint --clear --quiet --cache --ext .js,.ts.",
		"lintcheck": "eslint --quiet --cache --ext .js,.ts.",
		"typecheck": "tsc",
		"commitlint": "commitlint --edit",
		"sonar": "sonar-scanner",
		"prepare": "npx husky install & npx lint-staged"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npx lint-staged --concurrent false",
			"pre-push": "npx lint-staged -concurrent false"
		}
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix .",
			"prettier . --write"
		]
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~17.0.6",
		"@angular/cdk": "^17.0.3",
		"@angular/common": "~17.0.6",
		"@angular/compiler": "~17.0.6",
		"@angular/core": "~17.0.6",
		"@angular/forms": "~17.0.6",
		"@angular/material": "^17.0.3",
		"@angular/platform-browser": "~17.0.6",
		"@angular/platform-browser-dynamic": "~17.0.6",
		"@angular/router": "~17.0.6",
		"@antfu/eslint-config": "^2.4.3",
		"@ctrl/ngx-emoji-mart": "^9.2.0",
		"@jamesives/github-pages-deploy-action": "^4.1.4",
		"@microsoft/signalr": "^5.0.10",
		"bootstrap": "^5.0.2",
		"crypto-js": "^4.1.1",
		"eslint-plugin-regex": "^1.10.0",
		"express": "^4.17.1",
		"path": "^0.12.7",
		"rxjs": "~7.8.1",
		"stylelint-order-config-standard": "^0.1.3",
		"tslib": "^2.0.0",
		"typescript": "~5.2.2",
		"zone.js": "~0.14.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^17.0.6",
		"@angular-eslint/builder": "17.1.1",
		"@angular-eslint/eslint-plugin": "17.1.1",
		"@angular-eslint/eslint-plugin-template": "17.1.1",
		"@angular-eslint/schematics": "17.1.1",
		"@angular-eslint/template-parser": "17.1.1",
		"@angular/cli": "~17.0.6",
		"@angular/compiler-cli": "~17.0.6",
		"@commitlint/config-conventional": "^17.7.0",
		"@types/crypto-js": "^4.0.2",
		"@types/jasmine": "~3.6.0",
		"@types/node": "^12.11.1",
		"@typescript-eslint/eslint-plugin": "6.13.1",
		"@typescript-eslint/parser": "6.13.1",
		"autoprefixer": "^9.7.5",
		"browser-sync": "^2.26.7",
		"codelyzer": "^6.0.0",
		"commitlint": "^17.7.2",
		"cssnano": "^4.1.10",
		"electron": "^14.0.0",
		"enhanced-resolve": "^5.8.0",
		"eslint": "^8.54.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~6.4.2",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.0.3",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.6.0",
		"lint-staged": "^14.0.1",
		"protractor": "~7.0.0",
		"sonar-scanner": "^3.1.0",
		"stylelint": "^16.0.1",
		"stylelint-config-standard": "7.0.0",
		"stylelint-less": "^2.0.0",
		"stylelint-order": "^6.0.4",
		"stylelint-scss": "^5.3.1",
		"ts-node": "~10.9.2",
		"tslint": "~6.1.0"
	},
	"engines": {
		"node": "21.2.0",
		"npm": "10.2.3"
	},
	"deploy": "ng build --prod --base-href=/razumovskii.github.io/"
}
