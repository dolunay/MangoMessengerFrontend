/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "./out-tsc/spec",
		"types": ["jasmine"],
		"paths": {
			"@types/*": ["src/types/*"],
			"@core/*": ["src/core/*"],
			"@shared/*": ["src/shared/*"]
		}
	},
	"files": ["src/test.ts", "src/polyfills.ts"],
	"include": ["src/**/*.spec.ts", "src/**/*.d.ts"]
}
